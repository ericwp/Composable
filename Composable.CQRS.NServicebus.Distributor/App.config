<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="Logging" type="NServiceBus.Config.Logging, NServiceBus.Core" />
    <section name="MessageForwardingInCaseOfFaultConfig" type="NServiceBus.Config.MessageForwardingInCaseOfFaultConfig, NServiceBus.Core" />
  </configSections>
  <appSettings>
    <add key="NumberOfWorkerThreads" value="1" />
    <add key="DataInputQueue" value="distributorDataBus" />
    <add key="ControlInputQueue" value="distributorControlBus" />
    <add key="ErrorQueue" value="error" />
    <add key="StorageQueue" value="distributorStorage" />
    <add key="NameSpace" value="http://www.UdiDahan.com" />
    <!-- relevant for a Serialization of "interfaces" or "xml" -->
    <add key="Serialization" value="xml" />
    <!-- can be either "xml", or "binary" -->
  </appSettings>
  <Logging Threshold="WARN" />
  <runtime><assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
  <dependentAssembly>
    <Paket>True</Paket>
    <assemblyIdentity name="NServiceBus" publicKeyToken="9fc386479f8a226c" culture="neutral" />
    <bindingRedirect oldVersion="0.0.0.0-999.999.999.999" newVersion="4.7.0.0" />
  </dependentAssembly>
  <dependentAssembly>
    <Paket>True</Paket>
    <assemblyIdentity name="NServiceBus.Core" publicKeyToken="9fc386479f8a226c" culture="neutral" />
    <bindingRedirect oldVersion="0.0.0.0-999.999.999.999" newVersion="4.7.0.0" />
  </dependentAssembly>
  <dependentAssembly>
    <Paket>True</Paket>
    <assemblyIdentity name="NServiceBus.Host" publicKeyToken="9fc386479f8a226c" culture="neutral" />
    <bindingRedirect oldVersion="0.0.0.0-999.999.999.999" newVersion="4.7.0.0" />
  </dependentAssembly>
</assemblyBinding></runtime>
  <MessageForwardingInCaseOfFaultConfig ErrorQueue="error" />
</configuration>